$(
 *  TITLE               SHOEMASTER TABLET
 *  FUNCTION            Set up tablet information for shoemaster
 *                      
 *  AUTHOR              Patrick Brooke  7 Nov   1996 Cambridge  
 *  MODIFICATIONS
 *
$)

tablet define newsumm 16 1000 '+%7f,+%7f,%2d' last
tablet define oldsumm 16   10 '@%1x+%5f+%5f'  first
tablet define tds     16 1000 '+%7f,+%7f,%2d' last
tablet define 4       16  100 '%5f,%5f,%1x'   last
tablet define 5       16  100 '%d%f%f%f'      first  threed
tablet define 6       16  100 '%d%f%f%f'      first  threed
tablet define 7 16 40 '+%7f,+%7f,%2d' last
tablet define 8 4  100 '%5f,%5f,%1d' last
tablet define 9       16  100 '%d%f%f%f'      first  threed
tablet define 10 16 1000 '+%7f,+%7f,%2d' last
tablet define 20 16 1 'MICROSCRIBE' last threed
tablet define 21 16 1000 '+%7f,+%7f,%2d' last
tablet define 22 16 0.0393700787 'MICROSCRIBE' last
tablet define 23 16 0.0393700787 'MICROSCRIBE' last
tablet using oldsumm
tablet button  1  'XY'
tablet button  4  '$$p button * '
tablet button  5  'accept;disable tablet'
tablet button  6  'quit;new line twod'
tablet button  7  'quit;disable tablet'
tablet button  8  '$$p button 0 '
tablet button  9  '$$p button 8 '
tablet button 10  'disable tablet'
tablet button 11  'accept;display on line current;disable tablet'
tablet button 12  '$$p button # '
tablet button 13  '$$p button 9 '
tablet button 14  'page;display on line all'
tablet button 15  'accept;display on line current;new line twod'

tablet using newsumm
tablet button  1  'XY'
tablet button  2  ''
tablet button  3  'reference point ''Bottom Toe'' position $$( enter the toe point $$) $$'
tablet button  4  'reference point ''Bottom Heel'' position $$( enter the heel point $$) $$'
tablet button  5  'accept;disable tablet'
tablet button  6  'quit;new line twod'
tablet button  7  'quit;disable tablet'
tablet button  8  'reference point ''Bottom Outer'' position $$( enter the outer point $$) $$'
tablet button  9  'new line twod'
tablet button 10  'disable tablet'
tablet button 11  'accept;display on line current;disable tablet'
tablet button 12  'reference point ''Bottom Inner'' position $$( enter the inner point $$) $$'
tablet button 14  'page;display on line all'
tablet button 15  'accept;display on line current;new line twod'
tablet button 16  'accept bottom;page;enable hidden'


