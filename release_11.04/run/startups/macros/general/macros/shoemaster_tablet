tablet using 20
tablet button 1 'echo ''now digitize the inside last girth point'';reference point id Shoemaster ''InsideLastGradingPoint'' position $$'
tablet button 2 'echo ''now digitize the centre last girth point'';reference point id Shoemaster ''CentreLastGradingPoint'' position $$'
tablet button 3 'echo ''now digitize the outside last girth point'';reference point id Shoemaster ''OutsideLastGradingPoint'' position $$'
tablet button 4 'echo ''now digitize the inside backheight'';reference point id Shoemaster ''BackHeight'' position $$'
tablet button 5 'echo ''now digitize vamp point "a"'';reference point ''Vamp Point A'' position $$'
tablet button 6 'echo ''now digitize the inside underankle height'';reference point id TNO ''MedialAnkleheightPoint'' position $$'
tablet button 7 'echo ''now digitize the outside underankle height'';reference point id TNO ''LateralAnkleheightPoint'' position $$'
tablet button 8 'delete;page;redraw'
tablet button 9 'bearing -45 elevation 20 centre;redraw'
tablet button 10 'bearing 45 elevation 20 centre;redraw'
tablet button 11 'bearing 0 elevation 90 centre;redraw'
tablet button 12 'echo ''now digitize the line'';surface current last;new line xyz'
tablet button 13 'accept;display on line current;echo ''now digitize the next line'';new line xyz'
tablet button 14 'accept;display on line current'
tablet button 15 'quit;disable tablet'
tablet button 16 '$$( button 16 $$)'

